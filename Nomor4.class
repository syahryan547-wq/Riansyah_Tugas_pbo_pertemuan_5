import java.util.Scanner;

public class Soal4 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        System.out.print("Masukkan banyak angka: ");
        int n = in.nextInt();

        int total = 0;

        for (int i = 1; i <= n*2; i += 2) {
            System.out.print(i + " ");
            total += i;
        }

        System.out.println("\nTotal penjumlahan = " + total);
    }
}